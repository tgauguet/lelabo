$(".count-tasks").html( "<%= escape_javascript (render 'tasks/count') %>")

$(function($){
  $(".progressBar").each(function(){
    $(this).progressbar({
      value: <%= @list.tasks.where(done: true).count %>,
      max: <%= @list.tasks.count %>
    });
  });
});
$(".cancel-btn").click(function(){
		$(".edittask").hide();
		$(".taskname").show();
});